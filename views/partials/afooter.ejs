<div id="scroll-to-top-container">
    <a class="btn rounded-circle border-0 scroll-to-top" href="#page-top" style="background-color: rgb(50,185,255); padding-top: 11px;">
      <i class="fas fa-angle-up"></i>
    </a>
</div>

<script>
    var button = document.querySelector('#scroll-to-top-container');

    window.addEventListener('scroll', function() {
      button.style.display = (window.scrollY > 0) ? "block" : "none";
    });

    // Hide the button if the page is already at the top when the page loads
    window.onload = function() {
      if (window.scrollY === 0) {
        button.style.display = "none";
      }
    };
</script>

